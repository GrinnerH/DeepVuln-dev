TARGET_CWE_ID=CWE-121
BASELINE_QUERY_PATH=


# ==== RAG Providers ====
# Default (LocalChroma): build local index from data/knowledge_base
RAG_PROVIDER=local_chroma
LOCAL_CHROMA_KB_DIR=./data/knowledge_base
LOCAL_CHROMA_PERSIST_DIR=./data/chroma_db
LOCAL_CHROMA_EMBEDDING_MODEL=all-MiniLM-L6-v2
LOCAL_CHROMA_TOP_K=8
LOCAL_CHROMA_CHUNK_SIZE=800

# RAGFlow (remote HTTP)
# RAG_PROVIDER=ragflow
# RAGFLOW_API_URL=https://your-ragflow-endpoint
# RAGFLOW_API_KEY=your-key
# RAGFLOW_PAGE_SIZE=10
# RAGFLOW_CROSS_LANGUAGES=en,zh

# Milvus
# RAG_PROVIDER=milvus
# MILVUS_URI=http://localhost:19530  # or ./milvus.db for Lite
# MILVUS_COLLECTION=documents
# MILVUS_EMBEDDING_PROVIDER=openai
# MILVUS_EMBEDDING_MODEL=text-embedding-ada-002
# MILVUS_EMBEDDING_API_KEY=your-key
# MILVUS_EMBEDDING_BASE_URL=https://api.openai.com/v1
# MILVUS_TOP_K=10
# MILVUS_AUTO_LOAD_EXAMPLES=false

# Qdrant
# RAG_PROVIDER=qdrant
# QDRANT_LOCATION=:memory:
# QDRANT_API_KEY=
# QDRANT_COLLECTION=documents
# QDRANT_EMBEDDING_PROVIDER=openai
# QDRANT_EMBEDDING_MODEL=text-embedding-ada-002
# QDRANT_EMBEDDING_API_KEY=your-key
# QDRANT_EMBEDDING_BASE_URL=https://api.openai.com/v1
# QDRANT_TOP_K=10
# QDRANT_AUTO_LOAD_EXAMPLES=false

# ==== Search ====
# SEARCH_API=tavily|infoquest|duckduckgo|brave_search|arxiv|searx|wikipedia
SEARCH_API=duckduckgo

# ==== CodeQL ====
# Override CLI path; fallback to system `codeql` if unset
CODEQL_CLI_PATH=/path/to/codeql
CODEQL_DATABASE_ROOT=.codeql/db
# Path(s) to QL packs/libraries, e.g. CodeQL home/qlpacks
CODEQL_SEARCH_PATH=/path/to/ql:/path/to/qlpacks

# ==== Agent / Graph ====
AGENT_RECURSION_LIMIT=25
MAX_SEARCH_RESULTS=6
ENFORCE_RESEARCHER_SEARCH=false

# ==== LLM ====
# Generic OpenAI-compatible settings (used by embedding/providers if needed)
OPENAI_API_KEY=your-openai-key
OPENAI_BASE_URL=https://api.openai.com/v1
# Optional model selection hints
OPENAI_MODEL=gpt-4o
OPENAI_EMBEDDING_MODEL=text-embedding-3-large
LLM_PROVIDER = "openai"
LC_OUTPUT_VERSION = "v0"

LANGSMITH_TRACING=true
LANGSMITH_ENDPOINT="https://api.smith.langchain.com"
LANGSMITH_API_KEY=""
LANGSMITH_PROJECT="DeepVuln"
